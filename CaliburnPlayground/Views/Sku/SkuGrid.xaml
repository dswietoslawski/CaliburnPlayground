<UserControl x:Class="CaliburnPlayground.Views.Sku.SkuGrid"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CaliburnPlayground.Views.Sku"
             xmlns:cal="http://www.caliburnproject.org"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <DataGrid x:Name="Skus" AutoGenerateColumns="True" SelectedItem="{Binding SelectedSku}" cal:Message.Attach="[Event Loaded] = [OnSkusLoaded()]"  />
    </Grid>
</UserControl>
